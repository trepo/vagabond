{
  "name": "vagabond-db",
  "version": "0.6.2",
  "description": "A Javascript Graph Database",
  "keywords": [
    "graph",
    "database"
  ],
  "main": "lib/Graph.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel -d lib/ src/",
    "coverage": "./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover _mocha -- --reporter dot",
    "doc": "./node_modules/.bin/jsdoc src/ --configure ./jsdoc.json",
    "prepublish": "npm run compile",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trepo/vagabond.git"
  },
  "author": "John Clark <john@trepo.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trepo/vagabond/issues"
  },
  "homepage": "https://github.com/trepo/vagabond",
  "dependencies": {
    "levelup": "^1.3.1",
    "memdown": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-istanbul": "^0.5.9",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.13",
    "babel-plugin-transform-es2015-parameters": "^6.3.13",
    "chai": "^3.4.1",
    "gulp": "^3.9.0",
    "gulp-gh-pages": "^0.5.4",
    "ink-docstrap": "^0.5.3",
    "jsdoc": "^3.4.0",
    "mocha": "^2.3.4"
  },
  "engines": {
    "node": ">= 5"
  }
}
