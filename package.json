{
  "name": "vagabond",
  "version": "0.3.1",
  "description": "A Javascript Graph Database",
  "main": "lib/Graph.js",
  "scripts": {
    "postinstall": "npm run compile",
    "test": "./node_modules/.bin/mocha --compilers js:babel/register",
    "coverage": "./node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover _mocha -- --reporter dot",
    "doc": "./node_modules/.bin/jsdoc src/ --configure ./jsdoc.json",
    "compile": "./node_modules/.bin/babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trepo/vagabond.git"
  },
  "author": "John Clark",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trepo/vagabond/issues"
  },
  "homepage": "https://github.com/trepo/vagabond",
  "peerDependencies": {
    "levelup": "^1.2.1",
    "memdown": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.x",
    "babel-istanbul": "^0.3.17",
    "chai": "^3.2.0",
    "ink-docstrap": "fisch0920/docstrap",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "levelup": "^1.2.1",
    "memdown": "^1.0.0",
    "mocha": "^2.3.3"
  }
}
